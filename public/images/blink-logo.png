  // Function to set the cookie
  function setCookie(name, value, days) {
      const expires = new Date();
      expires.setTime(expires.getTime() + days * 24 * 60 * 60 * 1000);
      document.cookie = name + '=' + value + ';expires=' + expires.toUTCString() + ';path=/';
  }

  // Function to handle accepting all cookies
  function acceptAllCookies() {
    setCookie('cookieConsent', 'true', 365);
    hideBanner();
    if (document.cookie.includes('cookieConsent')) {
      enableTracking();
    }
  }

  // Function to handle enabling tracking after consent
  function enableTracking() {
    gtag('consent', 'update', {
      'ad_storage': 'granted',
      'ad_user_data': 'granted',
      'ad_personalization': 'granted',
      'analytics_storage': 'granted'
    });
    
    gtag('config', 'G-V0GNFN3JTC');
  }

  // Function to handle accepting only essential cookies
  function rejectCookies() {
    setCookie('rejectCookies', 'true', 365);
     hideBanner();
  }

  // Function to hide the banner
  function hideBanner() {
      document.getElementById('cookieBanner').style.display = 'none';
  }

  // Add event listeners to the buttons
  document.getElementById('acceptAllButton').addEventListener('click', acceptAllCookies);
  document.getElementById('rejectAllButton').addEventListener('click', rejectCookies);

  // Show the banner if consent is not given
  function showBanner() {
      if (!document.cookie.includes('cookieConsent') && !document.cookie.includes('rejectCookies')) {
      	//document.getElementById('cookieBanner').style.display = 'flex';
      }
  }
  
  showBanner();
  
});

</script>

