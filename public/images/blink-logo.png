  function switchLanguage(page, language) {
    const languagePage = languageDictionary[page];
    if (languagePage && languagePage[language]) {
      return languagePage[language];
    }
    // Return the original page if the language mapping doesn't exist
    
    return page;
  }

  // Function to handle language switch on click
  function handleLanguageSwitch(event) {
    event.preventDefault();
    
    const selectedLanguage = this.getAttribute('data-lang');
    const currentPage = window.location.pathname;
    const switchedPage = switchLanguage(currentPage, selectedLanguage);
   
    window.location.href = switchedPage;
  }

